<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.mao.nomadeworkers"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name="com.orm.SugarApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="DATABASE"
            android:value="nomade_worker.db"/>
        <meta-data
            android:name="VERSION"
            android:value="2"/>
        <meta-data
            android:name="QUERY_LOG"
            android:value="true"/>
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="com.example"/>

        <activity
            android:name=".activities.InterventionListActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activities.ClientShowActivity">
        </activity>
        <activity android:name=".activities.ClientEditActivity">
        </activity>
        <activity android:name=".activities.ClientListActivity">
        </activity>
    </application>

</manifest>
